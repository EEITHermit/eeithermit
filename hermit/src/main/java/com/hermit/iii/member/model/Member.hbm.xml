<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hermit.iii.member.model.MemberVO" table="Member">

		<id name="memNO" column="memNO">
			<generator class="identity" />
		</id>

		<property name="memTel" column="memTel" />
		<property name="memAccount" column="memAccount" />
		<property name="memPwd" column="memPwd" />
		<property name="memName" column="memName" />
		<property name="memGender" column="memGender" />
		<property name="memEmail" column="memEmail" />
		<property name="memRegister" column="memRegister" insert="false" update="false" />
		<property name="memStatus" column="memStatus" />
		<property name="memInfract" column="memInfract" />
		<property name="memImage" column="memImage" />

		<set name="favs" table="Favorite" lazy="false" order-by="favNO asc">
			<key>
				<column name="memNO"></column>
			</key>
			<one-to-many class="com.hermit.iii.favorite.model.FavoriteVO" />
		</set>
	</class>
</hibernate-mapping>