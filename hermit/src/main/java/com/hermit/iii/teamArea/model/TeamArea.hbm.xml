<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hermit.iii.teamArea.model.TeamAreaVO" table="TeamArea">
			<composite-id>
			<key-many-to-one name="teamMemberListVO"
				class="com.hermit.iii.teammemberlist.model.TeamMemberListVO" lazy="false">
				<column name="businNO" />
			</key-many-to-one>
			<key-many-to-one name="boroughsVO"
				class="com.hermit.iii.boroughs.model.BoroughsVO" lazy="false">
				<column name="boroughNO" />
			</key-many-to-one>
			</composite-id>
		<property name="cityNO" column="cityNO" />
	</class>
</hibernate-mapping>
